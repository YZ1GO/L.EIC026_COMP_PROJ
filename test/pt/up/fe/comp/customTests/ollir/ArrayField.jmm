import io;
import Quicksort;

class CompileIntArrayExample extends Quicksort {

    int[] intArrayField;
    int index;
    int assignedValue;

    public int getArrayElement(int index) {
        return intArrayField[index];
    }

    public int setArrayElement(int index, int value) {
        assignedValue = value;
        intArrayField = new int[value];
        intArrayField[index] = assignedValue;
        return intArrayField[index];
    }

    public int[] initializeArray(int size) {
        intArrayField = new int[size];
        return intArrayField;
    }

    public int updateArrayElement(int[] intArrayField) {
        int index;
        int value;
        index = 0;
        value = 1;
        intArrayField = new int[2];
        intArrayField[index] = value;
        return intArrayField[index];
    }

    public static void main(String[] args) {
    }
}